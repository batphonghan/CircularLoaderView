GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/DownloadButton" "${PODS_ROOT}/Headers/Public/MBCircularProgressBar" "${PODS_ROOT}/Headers/Public/UAProgressView"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/DownloadButton" "${PODS_CONFIGURATION_BUILD_DIR}/MBCircularProgressBar" "${PODS_CONFIGURATION_BUILD_DIR}/UAProgressView"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/DownloadButton" -isystem "${PODS_ROOT}/Headers/Public/MBCircularProgressBar" -isystem "${PODS_ROOT}/Headers/Public/UAProgressView"
OTHER_LDFLAGS = $(inherited) -ObjC -l"DownloadButton" -l"MBCircularProgressBar" -l"UAProgressView" -framework "UIKit"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
